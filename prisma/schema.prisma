// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Storefront {
  id            Int     @default(autoincrement()) @id
  subdomain     String  @unique
  pubkey        String
  themeUrl      String
  theme         Json
}